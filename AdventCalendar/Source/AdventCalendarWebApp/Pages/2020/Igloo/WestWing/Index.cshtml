@page
@model AdventCalendarWebApp.Pages._2020.Igloo.WestWing.IndexModel
@{
    ViewData["Title"] = "West Wing";
    var addLinks = Model.HasMagnifier;
    var foo = addLinks ? "a" : "g";
    var bar = addLinks ? "href=WestWing/Material" : "";
}
<div class="btn-group-vertical d-block">
    <a asp-page="../Index" class="btn btn-light">Go back to the center of the igloo</a>
</div>
<hr />

@if (addLinks && !Model.Solved)
{
    <div class="alert alert-info">You found the magnifier in the drawer. What can we do with it?</div>
}

@if (Model.CanSolve)
{
    <p class="text-justify">
        Too bad this advent calendar is drawn in black and white only.
        I am unable to see the colors of the gift warp rolls.
        Is there another way to find out their colors?
    </p>
    @if (!ModelState.IsValid)
    {
        <div class="alert alert-danger">@ModelState[string.Empty].Errors[0].ErrorMessage</div>
    }
    else if (Model.Solved)
    {
        <div class="alert alert-success">
            Yes, you are a genious.
            Comparing the surface of the gift wrap rolls with item of known color delivered the solution.
            The magnifier in the drawer was very useful.
        </div>
    }

    @if (!Model.Solved)
    {
        <form method="post">
            <div class="row">
                @for (int i = 0; i < 5; i++)
                {
                    <div class="col-4">
                        <div class="form-group">
                            <label for="exampleFormControlInput1">Present No. @(i + 1)</label>
                            <select asp-for="SelectedWraps[i]" asp-items="Model.SelectableWraps" class="form-control">
                            </select>
                        </div>
                    </div>
                }
                <div class="col-12 mt-3">
                    <input type="submit" class="btn btn-lg btn-dark btn-block" value="Wrap the toys" />
                </div>
            </div>
        </form>
    }
}

